{
  "plugins": ["plugins/markdown"],
  "recurseDepth": 10,
  "opts": {
    "template": "./server/node_modules/docdash",
    "destination": "./docs/",
    "recurse": true
  },
  "source": {
    "include": ["fixerClient/src/screens", "server/controller"],
    "includePattern": ".+\\.js(doc|x)?$",
    "excludePattern": "(node_modules|__tests__|__mocks__)"
  },
  "sourceType": "module",
  "tags": {
    "allowUnknownTags": true,
    "dictionaries": ["jsdoc", "closure"]
  },
  "templates": {
    "cleverLinks": false,
    "monospaceLinks": false
  },
  "docdash": {
    "sort": true,
    "search": true,
    "collapse": false
  }
}
