    "jest-ctrf-json-reporter": "^0.0.9",
    "mongodb": "^6.10.0",
    "react-test-renderer": "^18.3.1",
    "typescript": "^5.3.3"
  },
  "jest": {
    "moduleNameMapper": {
      ".+\\.(css|styl|less|sass|scss|png|jpg|ttf|woff|woff2)$": "identity-obj-proxy",
      "^@expo/vector-icons$": "src/__mocks__/@expo/vector-icons.js"
    },
    "setupFiles": [
      "<rootDir>/jest.setup.js"
    ],
    "collectCoverageFrom": [],
    "coverageThreshold": {
      "global": {
        "branches": 60,
        "functions": 60,
        "lines": 60,
        "statements": 60
      }
    },
    "preset": "react-native",
    "transformIgnorePatterns": [
      "node_modules/(?!(expo-localization|expo-constants|expo-image-picker|@testing-library/dom|expo-modules-core|@react-native|react-native-maps|react-native|react-native-vector-icons|react-native-dropdown-picker)/)"
    ],
    "reporters": [
      "default",
      [
        "jest-ctrf-json-reporter",
        {}
      ]
    ]
  },
  "private": true
}
