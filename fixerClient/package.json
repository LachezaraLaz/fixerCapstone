          "@stream-io/flat-list-mvcp",
          "jwt-decode",
          "react-native-fs",
          "react-native-geolocation-service"
        ],
        "listUnknownPackages": false
      }
    }
  },
  "name": "mynewproject",
  "version": "1.0.0",
  "main": "expo/AppEntry.js",
  "scripts": {
    "start": "expo start",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "web": "expo start --web",
    "test-coverage": "jest --coverage",
    "test": "jest"
  },
  "dependencies": {
